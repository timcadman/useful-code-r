@article{kiernan2008,
author = {Kiernan, Kathleen E. and Huerta, M. Carmen},
title = {Economic deprivation, maternal depression, parenting and children's cognitive and emotional development in early childhood1},
journal = {The British Journal of Sociology},
volume = {59},
number = {4},
pages = {783-806},
keywords = {Poverty, maternal depression, parenting, cognitive development, behaviour problems, Structural Equation Modelling},
doi = {10.1111/j.1468-4446.2008.00219.x},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1468-4446.2008.00219.x},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1468-4446.2008.00219.x},
abstract = {Abstract This study uses data from the UK Millennium Cohort Study to examine the extent to which economic circumstances in infancy and mother's mental well-being are associated with children's cognitive development and behaviour problems at age 3 years, and what part parenting behaviours and attitudes play in mediating these factors. The analyses derived from Structural Equation Modelling show that economic deprivation and maternal depression separately and collectively diminish the cognitive and emotional well-being of children, and part of this diminution emanates from less nurturing and engaged parenting by those with less economic and emotional resources.},
year = {2008}
}

